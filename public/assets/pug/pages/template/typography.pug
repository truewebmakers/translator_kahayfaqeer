- var sidebar                = true;
- var theme_customizer       = true;
- var slick                  = true;
- var scrollbar              = true;
include ../../components/mixin
doctype html
html(lang='en') 
  include ../../components/header-files
  body
    include ../../components/taptop
    include ../../components/loader
    #pageWrapper.page-wrapper.compact-wrapper
        include ../../components/header/header
        .page-body-wrapper
          include ../../components/sidebar                          
          .page-body
            .container-fluid               
                .page-title
                    .row
                        .col-sm-6.col-12
                          h2
                              | Typography
                        .col-sm-6.col-12
                          ol.breadcrumb
                              li.breadcrumb-item
                                a(href='index.html')                                        
                                  i.iconly-Home.icli.svg-color
