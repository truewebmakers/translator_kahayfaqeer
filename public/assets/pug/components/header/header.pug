header.page-header.row
  .logo-wrapper.d-flex.align-items-center.col-auto
    a(href="index.html")
      img.light-logo.img-fluid(src="../assets/images/logo/logo1.png" alt="logo")
      img.dark-logo.img-fluid(src="../assets/images/logo/logo-dark.png" alt="logo")
    a.close-btn.toggle-sidebar(href="javascript:void(0)")
        +IconlySprite('Category', 'svg-color')
  .page-main-header.col
    .header-left
      form(action='#', method='get').form-inline.search-full.col
        .form-group.w-100
          .Typeahead.Typeahead--twitterUsers
            .u-posRelative
              input.demo-input.Typeahead-input.form-control-plaintext.w-100(type='text' placeholder='Search Admiro ..' name='q' title='' autofocus)
              .spinner-border.Typeahead-spinner(role='status')
                span.sr-only Loading...
              i(data-feather='x').close-search
            .Typeahead-menu
      .form-group-header.d-lg-block.d-none
        .Typeahead.Typeahead--twitterUsers
          .u-posRelative.d-flex.align-items-center 
              input.demo-input.py-0.Typeahead-input.form-control-plaintext.w-100(type='text' placeholder='Type to Search...' name='q' title='')
              i.search-bg.iconly-Search.icli
    .nav-right
      ul.header-right  
        include language  
        li.search.d-lg-none.d-flex 
          a(href="javascript:void(0)")
            +IconlySprite('Search', '')
        li   
          a.dark-mode(href="javascript:void(0)")
            +IconlySprite('moondark', '')
        include cart
        include notification
        li
          a.full-screen(href="javascript:void(0)")  
            +IconlySprite('scanfull', '')  
        include bookmark
        li.cloud-design
          a.cloud-mode
            svg#cloudDrizzle.climacon.climacon_cloudDrizzle(version='1.1', viewBox='15 15 70 70')
